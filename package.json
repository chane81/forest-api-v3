{
  "name": "forest-api",
  "private": true,
  "description": "aws-serverless-express boilerplate",
  "version": "1.0.0",
  "author": {
    "name": "LEE"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf .build/**/* .serverless/**/*",
    "docker-build": "docker build -t forest-api ./",
    "docker-run": "docker run -d --name forest-api -p 4000:4000 forest-api:latest",
    "dcbuild": "docker-compose build",
    "dcup": "docker-compose up --build",
    "dev": "cross-env NODE_ENV=development  webpack --watch --color --config ./webpack/webpack.dev.js",
    "build-dev": "webpack --config ./webpack/webpack.dev.js",
    "build": "webpack --config ./webpack/webpack.prod.js",
    "start": "node ./dist/server.js",
    "deploy": "eb deploy ForestApi-env-1"
  },
  "dependencies": {
    "aws-sdk": "^2.800.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mssql": "^1.5.0",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv-webpack": "^6.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "generic-pool": "^3.7.1",
    "joi": "^17.2.1",
    "lodash": "^4.17.20",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "source-map-support": "^0.5.19",
    "tedious": "^9.2.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.4.0",
    "webpack-node-externals": "^2.5.2"
  },
