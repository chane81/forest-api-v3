    "cross-env": "^7.0.2",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "nodemon-webpack-plugin": "^4.3.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63"
  }
